    header(popover-close exclude-class="exclude")
      .header-content(ng-controller='indexController')
        .header-content-inner
          h1.smallerh1 GIÚP VIỆC CHUYÊN NGHIỆP
          h2 Tìm người giúp việc trong TP.HCM
          div
            .form-group
              input#btntheongay.btn.btn-danger.form-control.sb.longBtn.active(type='button', value='Theo ngày')
              input#btndaihan.btn.btn-danger.form-control.sb.longBtn(type='button', value='Dài hạn')
            div#theongay
              form#formTheoNgay.form-inline.removedot(action='/search')
                input.form-control.sb.typeahead(type='hidden', name='typesearch', value='nh')
                ul#nhlist
                  li
                    .form-group
                        select.form-control.sb.fixwquan.selectpicker(ng-model="data.quan")
                          option(value='' disabled) Chọn Quận
                          option(ng-repeat="quan in data.danhsachquan" value="{{quan}}"){{quan}}
                        input#quannh.form-control.sb.typeahead(type='hidden', name='quan', value='{{data.quan}}')

                        select.form-control.sb.fixwdichvu.selectpicker(ng-model="data.dichvu")
                          option(ng-repeat="dichvu in data.danhsachdichvu" value="{{dichvu}}"){{dichvu}}
                        input#dichvunh.form-control.sb.typeahead(type='hidden', name='dichvu', value='{{data.dichvu}}')

                        input.form-control.sb.dpindex.fixWTimeNH(type='text', name='ngay', placeholder='Chọn ngày', readonly='readonly', autocomplete='off')
                    .form-group
                        select.form-control.sb.fixWTime.selectpicker(ng-model="data.giobd1")
                          option(value='' disabled) Từ giờ
                          option(ng-repeat="option in data.availableOptions" value="{{option.id}}"){{option.name}}
                        input#gbdnh.form-control.sb.typeahead(type='hidden', name='giobd1', value='{{data.giobd1}}')

                        select.form-control.sb.fixWTime.selectpicker(ng-model="data.giokt1")
                          option(value='' disabled) Đến giờ
                          option(ng-repeat="option in data.availableOptions" value="{{option.id}}"){{option.name}}
                        input#gktnh.form-control.sb.typeahead(type='hidden', name='giokt1', value='{{data.giokt1}}')

                      input.form-control.sb.btn.btn-danger(id='submitnh1' type='submit', value='Tìm kiếm')
                  
            div#daihan.hide
              form#formDaiHan.form-inline.removedot(action='/search')
                input.form-control.sb.typeahead(type='hidden', name='typesearch', value='dh')
                ul#dhlist
                  li
                    .form-group
                        select.form-control.sb.fixwquan.selectpicker(ng-model="data.quan")
                          option(value='' disabled) Chọn Quận
                          option(ng-repeat="quan in data.danhsachquan" value="{{quan}}"){{quan}}
                        input#quandh.form-control.sb.typeahead(type='hidden', name='quan', value='{{data.quan}}')

                        select.form-control.sb.fixwdichvu.selectpicker(ng-model="data.dichvu")
                          option(value='' disabled) Chọn dịch vụ
                          option(ng-repeat="dichvu in data.danhsachdichvu" value="{{dichvu}}"){{dichvu}}
                        input#dichvudh.form-control.sb.typeahead(type='hidden', name='dichvu', value='{{data.dichvu}}')

                        input.form-control.sb.dpindex.fixWTime(type='text', name='ngaybd', placeholder='Từ ngày', readonly='readonly', autocomplete='off')
                        input.form-control.sb.dpindex.fixWTime(type='text', name='ngaykt', placeholder='Đến ngày', readonly='readonly', autocomplete='off')

                    .form-group
                        select.form-control.sb.fixWTime.selectpicker(ng-model="data.giobd1")
                          option(value='' disabled) Từ giờ
                          option(ng-repeat="option in data.availableOptions" value="{{option.id}}"){{option.name}}
                        input#gbddh.form-control.sb.typeahead(type='hidden', name='giobd1', value='{{data.giobd1}}')

